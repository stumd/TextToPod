{
  "name": "podcast-from-text",
  "version": "1.0.0",
  "description": "A TypeScript app to convert text to speech and create a podcast feed",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm src/index.ts",
    "build": "tsc",
    "dev": "nodemon --exec node --loader ts-node/esm src/index.ts"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "audio-buffer-utils": "^5.1.2",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.3",
    "mp3-concat": "^0.1.2",
    "multer": "^1.4.5-lts.1",
    "rss": "^1.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.4",
    "@types/rss": "^0.0.32",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.0",
    "typescript": "^5.3.3"
  }
}
